{
  "name": "@dictadata/storage-junctions",
  "version": "0.9.113",
  "description": "Node.js library for normalized data storage access and streaming transfers.",
  "author": "Drew O. Letcher",
  "license": "MIT",
  "keywords": [
    "dictadata",
    "storage",
    "junctions"
  ],
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "index.js",
  "exports": {
    ".": "./storage/index.js",
    "./package.json": "./package.json",
    "./types": "./storage/types/index.js",
    "./types/*": "./storage/types/*.js",
    "./types/*.js": "./storage/types/*.js",
    "./lib": "./lib/index.js",
    "./lib/*": "./lib/*.js",
    "./lib/*.js": "./lib/*.js",
    "./utils": "./lib/utils/index.js",
    "./utils/*": "./lib/utils/*.js",
    "./utils/*.js": "./lib/utils/*.js",
    "./test": "./lib/test/index.js",
    "./test/*": "./lib/test/*.js",
    "./test/*.js": "./lib/test/*.js"
  },
  "bin": {
    "st_launcher": "lib/test/st_launcher.js",
    "st_node_client": "lib/test/st_node_client.js"
  },
  "scripts": {
    "lint": "eslint storage/**/*.js",
    "lint-fix": "eslint --fix storage/**/*.js",
    "test": "testrunner.bat"
  },
  "directories": {
    "doc": "docs",
    "lib": "storage",
    "test": "test",
    "example": "test/lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dictadata/storage-junctions.git"
  },
  "homepage": "https://github.com/dictadata/storage-junctions#readme",
  "bugs": {
    "url": "https://github.com/dictadata/storage-junctions/issues"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "eslint": "^9.1.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.13.1",
    "basic-ftp": "^5.0.4",
    "form-data": "^4.0.0",
    "mysql2": "^3.10.0",
    "node-html-parser": "^6.1.13",
    "node-stream-zip": "^1.15.0",
    "shapefile": "^0.6.6",
    "sqlstring": "^2.3.3",
    "stream-chain": "^2.2.4",
    "stream-csv-as-json": "^1.0.4",
    "stream-json": "^1.8.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
